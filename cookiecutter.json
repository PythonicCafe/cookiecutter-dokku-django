{
  "_extensions": ["extensions.CustomFilters"],
  "project_name": "My Project",
  "project_slug": "{{ cookiecutter.project_name|slug }}",
  "apt_extra_packages": "",
  "gunicorn_workers": 4,
  "dokku_web_workers": 1,
  "max_line_length": 120,
  "enable_django_extensions": ["y", "n"],
  "enable_celery": ["y", "n"],
  "celery_workers_production": 2,
  "celery_workers_local": 2,
  "enable_mailhog": ["y", "n"],
  "enable_minio": ["y", "n"],
  "enable_redis": ["y", "n"],
  "enable_sentry": ["y", "n"],
  "mailhog_image": "mailhog/mailhog",
  "mailhog_version": "latest",
  "minio_image": "minio/minio",
  "minio_version": "latest",
  "database_software": ["postgres", "mariadb"],
  "db_image": "{% if cookiecutter.database_software == 'postgres' %}postgres{% elif cookiecutter.database_software == 'mariadb' %}mariadb{% endif %}",
  "db_version": "{% if cookiecutter.database_software == 'postgres' %}17.6-trixie{% elif cookiecutter.database_software == 'mariadb' %}11{% endif %}",
  "postgres_fts_utils": ["y", "n"],
  "db_shm_size": "2g",
  "python_image": "python",
  "python_version": "3.13-slim-trixie",
  "redis_image": "redis",
  "redis_version": "6.2-alpine",
  "author": "Your Name",
  "copyright_year": "2025",
  "license": ["GPL 3", "LGPL 3", "AGPL 3", "Apache 2.0", "MIT", "BSD", "Closed source"],
  "use_openai_client": ["y", "n"],
  "use_hugging_face": ["y", "n"]
}
